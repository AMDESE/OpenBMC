 	/*M0-M7*/	"","","","","","","","",
-	/*N0-N7*/	"","","","","","","","ASSERT_BMC_READY",
-	/*O0-O7*/	"","","HDT_SEL","","","JTAG_TRST_N","","",
+	/*N0-N7*/	"","","","","","","PSP_SOFT_FUSE_NOTIFY","ASSERT_BMC_READY",
+	/*O0-O7*/	"","","HDT_SEL","HDT_XTRIG5","HDT_XTRIG6","JTAG_TRST_N","","",
