From bed0696d04b4a6536723b3e5ca968b07e48c7c62 Mon Sep 17 00:00:00 2001
From: Vinu Vaghasia <vinu.vaghasia@amd.com>
Date: Fri, 12 Aug 2022 12:02:19 -0500
Subject: [PATCH 1/1] linux-aspeed: Add sys_inlet temp sensor with Titanite dts

SYS_INLET temp sensor addition with Titanite DTS

Signed-off-by: Vinu Vaghasia <vinu.vaghasia@amd.com>
---
 arch/arm/boot/dts/aspeed-bmc-amd-titanite.dts | 11 +++++++++++
 1 file changed, 11 insertions(+)

diff --git a/arch/arm/boot/dts/aspeed-bmc-amd-titanite.dts b/arch/arm/boot/dts/aspeed-bmc-amd-titanite.dts
index c9ce6bf393bb..d61f7bbbc94e 100644
--- a/arch/arm/boot/dts/aspeed-bmc-amd-titanite.dts
+++ b/arch/arm/boot/dts/aspeed-bmc-amd-titanite.dts
@@ -778,6 +778,17 @@
 				label = "p12v_aux";
 			};
 		};
+
+		i2c@6 {
+			#address-cells = <1>;
+			#size-cells = <0>;
+			reg = <6>;
+
+			lm75a@4a {	// SYS INLET
+				compatible = "national,lm75a";
+				reg = <0x4a>;
+			};
+		};
 	};
 };

--
